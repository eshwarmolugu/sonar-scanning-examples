  agent any
  tools {
       maven 'Maven'
        }
  stages {
       stage('Build') {
           steps {
		echo 'Stage Build'
		echo "$pwd"
		echo "${env.WORKSPACE}"
		npm install 
		//npm install
                //sh 'mvn -B -DskipTests clean compile'
                }
            }
       stage('Test') {
           steps {
		echo 'Stage Test'
                //sh 'mvn test'    
	            }
            }
      stage('Deploy') {
           steps {
              echo 'Steage Deploy '
                }
            }
        }
}
