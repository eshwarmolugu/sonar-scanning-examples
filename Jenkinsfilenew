pipeline {
  agent any
  tools {
       maven 'Maven'
        }
  stages {
       stage('CheckOut') {
	steps{
	git credentialsId: 'eshwar_codedevsnc', url: 'https://github.com/eshwarmolugu/sonar-scanning-examples.git'
}
		}
       stage('Build') {
           steps {
		echo 'Stage Build'
		echo "$pwd"
		echo "${env.WORKSPACE}"

		nodejs('nodejseshwar') {
		   sh "npm -v"
		   sh "npm install ."
		}
		docker('dockereshwar') {
		   sh "docker -v"
		}
                }
            }
       stage('Test') {
           steps {
		echo 'Stage Test'
                //sh 'mvn test'    
	            }
            }
      stage('Deploy') {
           steps {
              echo 'Steage Deploy '
                }
            }
        }
}
