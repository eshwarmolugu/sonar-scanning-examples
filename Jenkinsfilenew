pipeline {
  agent any
  tools {
       maven 'Maven'
	dockerTool 'dockereshwar'	
        }
  stages {
       stage('CheckOut') {
	steps{
	git credentialsId: 'eshwar_codedevsnc', url: 'https://github.com/eshwarmolugu/sonar-scanning-examples.git'
}
		}
       stage('Build') {
           steps {
		echo 'Stage Build'
		echo "$pwd"
		echo "${env.WORKSPACE}"

		nodejs('nodejseshwar') {
		   sh "npm -v"
		   sh "npm install ."
		}
		sh "docker -v"
                }
            }
       stage('Test') {
           steps {
		echo 'Stage Test'
		withDockerContainer(image: 'any', toolName: 'dockereshwar') {
  		sh "docker -v"
		}
 		
	        }
            }
      stage('Deploy') {
           steps {
              echo 'Steage Deploy '
                }
            }
        }
}
